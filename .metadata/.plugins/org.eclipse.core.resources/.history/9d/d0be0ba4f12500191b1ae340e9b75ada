import java.util.ArrayList;

public class ListClassDemo {

	public static void arrayListClassDemo() {
		 ArrayList<String> strList=new ArrayList<>();
		  //insert
		  
		  strList.add("Satish");
		  strList.add("Kumar");
		  strList.add("Nilesh");
		  strList.add("Rakesh");
		  strList.add("Ayush");
		  strList.add("Satish");
		  strList.add("Mayur");
		  
		  System.out.println(strList);
		  
		  //search
		  
		 String nameToBeSearch="Ayush";
		 System.out.println((strList.contains(nameToBeSearch));
		 
		 //remove
		 String nameToRemove="Kumar";
		 System.out.println((strList.remove(nameToBeSearch));
		 //sort
		 
		 //iterate

	}

}
